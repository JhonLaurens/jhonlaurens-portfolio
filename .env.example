# Vercel Environment Variables Example
# Copy this file to .env.local for local development

# Application Settings
NODE_ENV=production
NEXT_PUBLIC_APP_NAME=jhonlaurens-portfolio
NEXT_PUBLIC_APP_VERSION=1.0.1

# Vercel System Variables (automatically set by Vercel)
# VERCEL_ENV=production
# VERCEL_URL=jhonlaurens.vercel.app
# VERCEL_REGION=iad1
# VERCEL_GIT_COMMIT_SHA=
# VERCEL_GIT_COMMIT_MESSAGE=
# VERCEL_GIT_COMMIT_AUTHOR_NAME=
# VERCEL_GIT_REPO_SLUG=jhonlaurens-portfolio
# VERCEL_GIT_REPO_OWNER=jhonlaurens

# API Configuration
API_BASE_URL=https://jhonlaurens.vercel.app/api
API_TIMEOUT=30000

# Contact Form Settings (if using external service)
# CONTACT_FORM_ENDPOINT=
# CONTACT_FORM_API_KEY=
# EMAILJS_SERVICE_ID=
# EMAILJS_TEMPLATE_ID=
# EMAILJS_PUBLIC_KEY=

# Analytics (if using)
# GOOGLE_ANALYTICS_ID=
# GOOGLE_TAG_MANAGER_ID=
# HOTJAR_ID=

# SEO Settings
NEXT_PUBLIC_SITE_URL=https://jhonlaurens.vercel.app
NEXT_PUBLIC_SITE_NAME=Jhon Laurens Portfolio
NEXT_PUBLIC_SITE_DESCRIPTION=Portfolio profesional de Jhon Laurens - Data Engineer & AI Specialist

# Social Media
NEXT_PUBLIC_LINKEDIN_URL=https://linkedin.com/in/jhonlaurens
NEXT_PUBLIC_GITHUB_URL=https://github.com/jhonlaurens
NEXT_PUBLIC_EMAIL=jhonlaurens@gmail.com

# Performance Settings
NEXT_PUBLIC_ENABLE_ANALYTICS=true
NEXT_PUBLIC_ENABLE_SERVICE_WORKER=true
NEXT_PUBLIC_CACHE_DURATION=3600

# Security Settings
NEXT_PUBLIC_CSP_REPORT_URI=
NEXT_PUBLIC_ALLOWED_ORIGINS=https://jhonlaurens.vercel.app,https://jhonlaurens.dev